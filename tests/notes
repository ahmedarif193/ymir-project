#https://www.objgen.com/json/local/design

numberOfInstances n = 1
instances[]
  id n = 1
  name s = CONTAINER1
  type s = busybox
  pid n = 1234
  status = UP

{
  "numberOfInstances": 1,
  "instances": [
    {
      "id": 1,
      "name": "CONTAINER1",
      "type": "busybox",
      "pid": 1234,
      "status": "UP"
    }
  ]
}

curl -X GET http://localhost:8080/2.0/execenv

//test create and enable
curl -X POST http://localhost:8080/2.0/execenv \
   -H "Content-Type: multipart/form-data" \
   -d '{ "name": "Container-4", "type": "busybox", "enable": true }'

//test remove
curl -X DELETE http://localhost:8080/2.0/execenv \
   -H "Content-Type: multipart/form-data" \
   -d '{ "name": "Container-4" }'

